model Ticket {
  id          String   @id @default(uuid())
  title       String
  description String
  category    String
  priority    Int      @default(1)
  progress    Int      @default(0)
  status      String
  active      Boolean  @default(false)
  createdAt   DateTime @default(now()) @map("createdAt")
  updatedAt   DateTime @updatedAt @map("updatedAt")
}
